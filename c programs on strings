1.print only alphats from string
#include<stdio.h>
#include<string.h>
void remov(char str[],int l)
{
   for (int i = 0, j; str[i] != '\0'; i++)
   {
      while (!(str[i] >= 'a' && str[i] <= 'z') && !(str[i] >= 'A' && str[i] <= 'Z') && !(str[i] == '\0'))
      {
         for (j = i; str[j] != '\0';j++)
         {
            str[j] = str[j + 1];
         }
         str[j] = '\0';
      }
   }
   printf("Output String: ");
  
        printf("%s",str);   
 

    
}
int main()
{
    char str[]="hema123HEMa";
    // printf("enter the string:");
    // fgets(str,100,stdin);
    int l=strlen(str);
    void (*fptr)(char[],int);
    fptr=remov;
    fptr(str,l);
}



2. strings to check anagrams are not
#include<stdio.h>
#include<string.h>
int main()
{
    char str1[100];
    char str2[100];
    int i,j,temp,flag=0,len;
    printf("enter the first string:");
    scanf("%s",str1);
    printf("enter the second string:");
    scanf("%s",str2);
    int l1=strlen(str1);
    int l2=strlen(str2);
    if(l1==l2)
    {
        for(i=0;i<l1;i++)
        {
            for(j=0;j<l1;j++)
            {
                if(str1[i]>str1[j])
                {
                   temp=str1[i];
                   str1[i]=str1[j];
                   str1[j]=temp;
                }
            }
        }
       
           for(i=0;i<l2;i++)
            {
                for(j=0;j<l2;j++)
                {
                    if(str2[i]>str2[j])
                    {
                        temp=str2[i];
                        str2[i]=str2[j];
                        str2[j]=temp;
                    }
                 }
            }
        
        if(strcmp(str1,str2)==0)
        flag=1;
        else
        flag=2;
   }
   
       (flag==1)?printf("strings are anagrams"):printf("strings are not anagrams");
  return 0;
}
output:
enter the first string:dusty
enter the second string:study
strings are anagrams
