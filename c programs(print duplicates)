#include<stdio.h>
void duplicate(int arr[],int l)
{
    int i;
    for(i=0;i<l;i++)
    {
        arr[arr[i]%l]=arr[arr[i]%l]+l;
    }
    for(i=0;i<l;i++)
    {
        if(arr[i]>=l*2)
        printf("%d\n",i);
    }
}
int main()
{
    int arr[]={1,2,3,3,5,6,7,8,9,1,1,1,1,3,3,3,7};
    int l=sizeof(arr)/sizeof(arr[0]);
    void (*fptr)(int [],int);
    fptr=duplicate;
    fptr(arr,l);
}
output:
1
3
7
